<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :Artist %>
  <%= f.text_field :artist_name %>
  <br />
  <%= f.label :genre %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <br />

  <input name="song[song_notes_content][]" id='song_notes_1'><br>
  <input name="song[song_notes_content][]" id= 'song_notes_2'><br>

  <%= f.submit %>
<% end %>


